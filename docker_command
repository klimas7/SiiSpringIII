#mongo
docker pull mongo
docker run --name sii-mongo -p 0.0.0.0:27017:27017 -p 0.0.0.0:28017:28017 -d mongo:latest
docker logs sii-mongo

NETWORK  [initandlisten] waiting for connections on port 27017

docker exec -it sii-mongo bash
